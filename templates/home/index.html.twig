<main role="main">

    <!-- Ads & other things carousel -->
    <div id="carouselExampleIndicators" class="carousel slide col-lg-8 offset-lg-2" data-ride="carousel">
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img class="d-block w-100" src="https://i.ibb.co/9ZgZ0cN/carousel-2.png" alt="First slide">
            </div>
            <div class="carousel-item">
                <img class="d-block w-100" src="https://i.ibb.co/sVMDfv9/carousel-3.png" alt="Second slide">
            </div>
            <div class="carousel-item">
                <img class="d-block w-100" src="https://i.ibb.co/9VMK4SF/carousel-1.png" alt="Third slide">
            </div>
        </div>

        <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </a>
    </div>

    <!-- Best sellers carousel -->
    <div class="container my-4">
        <hr class="my-4">

        <!-- Wrapper -->
        <div id="multi-item-example" class="carousel slide carousel-multi-item" data-ride="carousel">
            <div id="TEST-BIDOUILLAGE" class="d-flex flex-row align-items-center">
                <div class="mx-2">
                    <a class="btn" href="#multi-item-example" data-slide="prev"><i class="fa fa-chevron-left fa-3x"></i></a>
                </div>

                <!-- Slides -->
                <div class="carousel-inner" role="listbox">

                    <!-- First slide -->
                    <div class="carousel-item active">
                        <div class="row">
                            {% for i in 0..2 %}
                                <div class="col-md-4">
                                    <div class="card mb-2">
                                        <img class="card-img-top" src="{{ articleImages[i].url }}" alt="Card image cap">
                                        <div class="card-body">
                                            <h4 class="card-title">{{ products[i].title }}</h4>
                                            <p class="card-text">{{ products[i].description }}</p>
                                            <a class="btn btn-primary">Button</a>
                                        </div>
                                    </div>
                                </div>
                            {% endfor %}
                        </div>
                    </div>

                    <!-- Second slide -->
                    <div class="carousel-item">
                        <div class="row">
                            {% for i in 3..5 %}
                                <div class="col-md-4">
                                    <div class="card mb-2">
                                        <img class="card-img-top" src="{{ articleImages[i].url }}" alt="Card image cap">
                                        <div class="card-body">
                                            <h4 class="card-title">{{ products[i].title }}</h4>
                                            <p class="card-text">{{ products[i].description }}</p>
                                            <a class="btn btn-primary">Button</a>
                                        </div>
                                    </div>
                                </div>
                            {% endfor %}
                        </div>
                    </div>

                    <!-- Third slide -->
                    <div class="carousel-item">
                        <div class="row">
                            {% for i in 6..7 %}
                                <div class="col-md-4">
                                    <div class="card mb-2">
                                        <img class="card-img-top" src="{{ articleImages[i].url }}" alt="Card image cap">
                                        <div class="card-body">
                                            <h4 class="card-title">{{ products[i].title }}</h4>
                                            <p class="card-text">{{ products[i].description }}</p>
                                            <a class="btn btn-primary">Button</a>
                                        </div>
                                    </div>
                                </div>
                            {% endfor %}
                        </div>
                    </div>
              </div>

              <div class="mx-2">
                  <a class="btn" href="#multi-item-example" data-slide="next"><i class="fa fa-chevron-right fa-3x"></i></a>
              </div>
            </div>
        </div>
    </div>
